<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Bed" table="bed">
    <id name="bedno" type="integer" column="bedNo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="status" type="string" column="status" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="oxygensypply" type="string" column="oxygenSypply" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="artificialrespiration" type="string" column="artificialRespiration" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cardiacmonitor" type="string" column="cardiacMonitor" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
