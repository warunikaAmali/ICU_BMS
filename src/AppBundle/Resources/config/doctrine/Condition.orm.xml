    <field name="noninvaisvemedication" type="string" column="nonInvaisveMedication" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dialysis" type="string" column="dialysis" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
